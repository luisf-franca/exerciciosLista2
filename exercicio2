import javax.swing.JOptionPane;

class Main {
  public static void main(String[] args) {

    Float media, nota1, nota2;
    nota1 = Float.parseFloat(JOptionPane.showInputDialog("Por favor, insira sua primeira nota"));
    nota2 = Float.parseFloat(JOptionPane.showInputDialog("Por favor, insira sua segunda nota "));

    media = (nota1 + nota2) / 2;
    if (media >= 7) {
      JOptionPane.showMessageDialog(null, "Aprovado!");
    } else if (media < 7) {
      JOptionPane.showMessageDialog(null, "Rprovado!");
    }
  }
}
